import imageio as iio

filenames=["6f210877-ec6c-45ff-a64a-f74a5d550172.png","cb8cde84-6b39-43b6-8c64-03a4b55f626c.png"]

images=[]

for filename in filenames:
    images.append(iio.imread(filename))

iio.mimsave("output.gif",images,duration =500,loop=0)


